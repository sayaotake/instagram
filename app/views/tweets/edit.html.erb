<%= form_for @tweet do |f| %>

    <div class="field">
      <%= f.label :行き先 %>
      <%= f.text_field :place, :size => 30 %>
    </div>
  
    <div class="field">
      <%= f.label :住所 %>
      <%= f.text_field :address, :size => 30 %>
    </div>
  
    <div class="field">
      <%= f.label :コメント  %>
      <%= f.text_area :review, :size => "30x2" %>
    </div>
  
    <%= f.submit "編集する" %>
  <% end %>